# CC = cc
# CC = clang
CC = gcc
CFLAGS = -Wall -O2
LIBS = -lm

all: ex13-1 ex13-2 ex13-3 ex13-4 q13-1 q13-2 q13-3 q13-4 

ex13-1: ex13-1.c
	$(CC) ex13-1.c $(CFLAGS) $(LIBS) -o ex13-1

ex13-2: ex13-2.c
	$(CC) ex13-2.c $(CFLAGS) $(LIBS) -o ex13-2

ex13-3: ex13-3.c
	$(CC) ex13-3.c $(CFLAGS) $(LIBS) -o ex13-3

ex13-4: ex13-4.c
	$(CC) ex13-4.c $(CFLAGS) $(LIBS) -o ex13-4

q13-1: q13-1.c
	$(CC) q13-1.c $(CFLAGS) $(LIBS) -o q13-1

q13-2: q13-2.c
	$(CC) q13-2.c $(CFLAGS) $(LIBS) -o q13-2

q13-3: q13-3.c
	$(CC) q13-3.c $(CFLAGS) $(LIBS) -o q13-3

q13-4: q13-4.c
	$(CC) q13-4.c $(CFLAGS) $(LIBS) -o q13-4

clean:
	rm -rf *~  ex13-1 ex13-2 ex13-3 ex13-4 q13-1 q13-2 q13-3 q13-4 

