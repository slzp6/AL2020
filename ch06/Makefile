# CC = cc
# CC = clang
CC = gcc
CFLAGS = -Wall -Wno-unused-result -O2 
LIBS = -lm

all: ex6-1 ex6-10 ex6-2 ex6-3 ex6-4 ex6-5 ex6-6 ex6-7 ex6-8 ex6-9 q6-1 q6-2 q6-3 q6-4 q6-5 q6-6 

