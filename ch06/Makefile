	$(CC) $(CFLAGS) ex6-4.c -o ex6-4 $(LIBS)

ex6-5: ex6-5.c
	$(CC) $(CFLAGS) ex6-5.c -o ex6-5 $(LIBS)

ex6-6: ex6-6.c
	$(CC) $(CFLAGS) ex6-6.c -o ex6-6 $(LIBS)

ex6-7: ex6-7.c
	$(CC) $(CFLAGS) ex6-7.c -o ex6-7 $(LIBS)

ex6-8: ex6-8.c
	$(CC) $(CFLAGS) ex6-8.c -o ex6-8 $(LIBS)

ex6-9: ex6-9.c
	$(CC) $(CFLAGS) ex6-9.c -o ex6-9 $(LIBS)

