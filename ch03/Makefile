# CC = cc
# CC = clang
CC = gcc
CFLAGS = -Wall -Wno-unused-result -O2 
LIBS = -lm

all: ex3-1 ex3-10 ex3-11 ex3-2 ex3-3 ex3-4 ex3-5 ex3-6 ex3-7 ex3-8 ex3-9 q3-1 q3-2 q3-3 q3-4 q3-5 

ex3-1: ex3-1.c
	$(CC) $(CFLAGS) ex3-1.c -o ex3-1 $(LIBS)

ex3-10: ex3-10.c
	$(CC) $(CFLAGS) ex3-10.c -o ex3-10 $(LIBS)

ex3-11: ex3-11.c
	$(CC) $(CFLAGS) ex3-11.c -o ex3-11 $(LIBS)

