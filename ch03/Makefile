	$(CC) $(CFLAGS) ex3-3.c -o ex3-3 $(LIBS)

ex3-4: ex3-4.c
	$(CC) $(CFLAGS) ex3-4.c -o ex3-4 $(LIBS)

ex3-5: ex3-5.c
	$(CC) $(CFLAGS) ex3-5.c -o ex3-5 $(LIBS)

ex3-6: ex3-6.c
	$(CC) $(CFLAGS) ex3-6.c -o ex3-6 $(LIBS)

ex3-7: ex3-7.c
	$(CC) $(CFLAGS) ex3-7.c -o ex3-7 $(LIBS)

ex3-8: ex3-8.c
	$(CC) $(CFLAGS) ex3-8.c -o ex3-8 $(LIBS)

ex3-9: ex3-9.c
	$(CC) $(CFLAGS) ex3-9.c -o ex3-9 $(LIBS)

q3-1: q3-1.c
	$(CC) $(CFLAGS) q3-1.c -o q3-1 $(LIBS)

q3-2: q3-2.c
	$(CC) $(CFLAGS) q3-2.c -o q3-2 $(LIBS)

q3-3: q3-3.c
	$(CC) $(CFLAGS) q3-3.c -o q3-3 $(LIBS)

q3-4: q3-4.c
	$(CC) $(CFLAGS) q3-4.c -o q3-4 $(LIBS)

q3-5: q3-5.c
